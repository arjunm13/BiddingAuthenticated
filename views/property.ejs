<div class="jumbotron bidding">
        <script>
function validateForm() {
    var x = document.forms["homebid"]["bidvalue"].value;
    var highestBid = <%= home.highestbidvalue %>;
    if (x <= highestBid || x == "") {
        alert("Not a Higher Bid!!");
        return false;
    }
}
</script>
       

 <form name="homebid" class="form-horizontal" role="form" onsubmit="return validateForm()" method="post" action="/homes/<%= home._id %>/bid">
    <div class="form-group">
      
        <label for="Bid" class="col-sm-2 control-label"></label>
        
             <h2 align="center">$<%= home.highestbidvalue %></h2>
                  <h5>Highest Bid</h5>
           
        

 
       <input type="text" class="form-control" id="bidvalue" name="bidvalue" placeholder="Enter Your Bid" value="">

         <div class="col-sm-10 col-sm-offset-2">
        <h5>  <input id="submit" name="submit" type="submit" value="BID" class="btn btn-primary"> </h5>
      </div> 
      </div></form></div>

