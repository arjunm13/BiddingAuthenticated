            <li data-target="#screenshot-carousel" data-slide-to="0" class="active"></li>
            <li data-target="#screenshot-carousel" data-slide-to="1"></li>
            <li data-target="#screenshot-carousel" data-slide-to="2"></li>
            <li data-target="#screenshot-carousel" data-slide-to="3"></li>
          </ol>
          <div class="carousel-inner">
            <div class="item active">
              <img src="https://upload.wikimedia.org/wikipedia/commons/b/bb/Brock_House.JPG" alt="Text of the image" height="500px" width="500px">
              <div class="carousel-caption">
                <h3>Highway heading</h3>
                <p>This is the caption</p>
              </div>
            </div>
            <div class="item">
              <img src="https://upload.wikimedia.org/wikipedia/en/8/8f/Colorful_house_in_the_town_of_Appenzell.JPG" alt="Text of the image" height="500px" width="500px">
              <div class="carousel-caption">
                <h3>River heading</h3>
                <p>This is the caption</p>
              </div>
            </div>
            <div class="item">
              <img src="https://upload.wikimedia.org/wikipedia/commons/b/b5/Edward_Gorey_House,_Yarmouth_Port_MA.jpg" alt="Text of the image" height="500px" width="500px" position="center" margin="auto">
              <div class="carousel-caption">
                <h3>Street heading</h3>
                <p>This is the caption</p>
              </div>
            </div>
            <div class="item">
              <img src="img/3011394376_8306820abc_o.jpg" alt="Text of the image">
              <div class="carousel-caption" height="500px" width="500px">
                <h3>Painting heading</h3>
                <p>This is the caption</p>
              </div>
            </div>

          </div><!-- End Carousel inner -->
          <a href="#screenshot-carousel" class="left carousel-control" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
          </a>
          <a href="#screenshot-carousel" class="right carousel-control" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
          </a>
        </div><!-- End Carousel -->

