<div class="jumbotron" id="rooms">
             
              
            
  <form class="form-horizontal" role="form" method="post" action="<%= home._id %>/bid">
    <div class="form-group">
      
        <label for="Bid" class="col-sm-2 control-label">BID</label>
        <div class="col-sm-10">
             <p>Highest Bid is <%= home.highestbidvalue %></p>
        </div>
    </div>
    <div class="form-group">

        <label for="Bid" class="col-sm-2 control-label">BID</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="bidvalue" name="bidvalue" placeholder="Enter Your Bid" value="">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-10 col-sm-offset-2">
            <input id="submit" name="submit" type="submit" value="Send" class="btn btn-primary">
        </div>
    </div>
</form>
</div>

</body>
<%if (user== "ADMIN") { %>
<div class="jumbotron" id="rooms">
  <h2>This house is <%= home.publicTrue%></h2>
  <form action = "/admin/verify/<%= home._id%>/true" method = "post">
        <input type="submit" name="setTrue" value="setTrue" />
  </form>
   <form action = "/admin/verify/<%= home._id%>/false" method = "post">
        <input type="submit" name="setFalse" value="setFalse" />
  </form>

</div>

<% } %>
