<% include layout.ejs %>
<div class="container">
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center login-title">CREATE A NEW HOUSE
        <div class="signup-wall">
          <form action="/homes/new" method="POST" class="form-signin">

            <input type="text" name="address" placeholder="address" required="required" class="form-control nomargin"/>
            <input type="text" name="price" placeholder="price" required="required" class="form-control"/>
            <input type="text" name="squareFoot" placeholder="squareFoot" required="required" class="form-control"/>
            <input type="text" name="rooms" placeholder="rooms" required="required" class="form-control"/>
            <button type="submit" class="btn btn-lg btn-primary btn-block">Create</button><span class="clearfix"></span>
          </form>
        </div>
        <div id="message">
        </div>
      </h1>
    </div>
  </div>
</div>

