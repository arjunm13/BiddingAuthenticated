<div class="list-group">

  	<% homes.forEach(function(home){ %>
  <a href="/homes/show/<%= home._id%>" class="list-group-item active" style="float: left">
  <img src="data:image/jpeg;base64," alt="Smiley face" height="42" width="42">
  <h4 class="list-group-item-heading" style="float: right"><%= home.address %></h4>
    <p class="list-group-item-text" ><%= home.price%></p>
    <p class="list-group-item-text"><%= home.rooms%></p>
    <%}) %>
  </a>
</div>


